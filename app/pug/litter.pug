extends blocks/_layout

append content
    main
        .breadcrumb-main.breadcrumb-dark
            nav(aria-label='breadcrumb')
                .container
                    ol(itemscope itemtype="http://schema.org/BreadcrumbList").breadcrumb.new.pl-0
                        li(itemprop="itemListElement" itemscope
                        itemtype="http://schema.org/ListItem").breadcrumb-item
                            a(href='#' itemprop="item")
                                span(itemprop="name") Главная
                                meta(itemprop="position" content="1")
                        li(itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem" aria-current='litter').breadcrumb-item.active
                            a(href='#' itemprop="item")
                                span(itemprop="name") Литтер
                                meta(itemprop="position" content="1")
        .title-page.mb-3
            .container
                .row
                    .col-12.mt-5
                        h1 Литтер
        .parents.mt-5
            .container
                .row
                    .col-12.mt-3.col-xl-6
                        .parent-outer
                            .row.d-flex.flex-column.flex-sm-column.flex-md-row.flex-xl-row
                                .col-12.col-sm-12.col-md-5.col-xl-5
                                    .parent-cat-img
                                        img(src='img/card-cats/cats.jpg')
                                .col-12.col-sm-7.col-md-7.col-xl-7
                                    .parent-cat-info
                                        div Мама
                                        div Alisia SiOriPraid
                                        a(href='#') Смотреть профиль

                    .col-12.mt-3.col-xl-6
                        .parent-outer
                            .row.d-flex.flex-column.flex-sm-column.flex-md-row.flex-xl-row
                                .col-12.col-sm-12.col-md-5.col-xl-5
                                    .parent-cat-img
                                        img(src='img/card-cats/cats.jpg')
                                .col-12.col-sm-7.col-md-7.col-xl-7
                                    .parent-cat-info
                                        div Папа
                                        div Viktor Gift Of Kings
                                        a(href='#') Смотреть профиль
        .galary.mt-3
            .container
                .row
                    .col-8
                        h4 Дата рождения 31.12.2018
                .row.mt-3.mt-xl-5
                    .col-12.col-sm-12.col-md-12.col-xl-6
                        .slick-slider-with-arrows
                            .carousel-item.active
                              img.d-block.w-100(src='img/slider/IMG_2161.jpg', alt='cat')
                            .carousel-item
                              img.d-block.w-100(src='img/slider/IMG_2161.jpg', alt='cat')
                            .carousel-item
                              img.d-block.w-100(src='img/slider/IMG_2161.jpg', alt='cat')
                    .col
                        .galary-info.mt-3
                            h4.mb-3 Hermes SiOri Praid
                            ul
                                li Пол:
                                    span Male
                                li Порода:
                                    span OSH
                                li Окрас:
                                    span Дым
                                li Статус:
                                    span Продается (смотреть)
                .row.mt-3.mt-xl-5
